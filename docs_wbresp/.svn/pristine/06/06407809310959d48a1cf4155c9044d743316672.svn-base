.btn-whlit,.btn-whlit:hover{color:#222;border-color:#8ab10b;background: #fff url(/wbresp/img/arrow-btn-white.png) right center no-repeat;text-decoration:none}
.btn-whlit:hover{border-color:#222;background: #fff url(/wbresp/img/arrow-btn-white-hover.png) right center no-repeat;}
.btn-gray-border{background-color:#2c2b2b}
.btn-gray-border:hover{color:#222;background: #fff url(/wbresp/img/arrow-btn-white-hover.png) right center no-repeat;border-color:#FFF;}
.btn-arrow{border-color:#fff}
.btn-arrow:hover{background: #2c2b2b  url(/wbresp/img/arrow-btn-green.png) right center no-repeat;border-color:#2c2b2b ;color:#fff}
