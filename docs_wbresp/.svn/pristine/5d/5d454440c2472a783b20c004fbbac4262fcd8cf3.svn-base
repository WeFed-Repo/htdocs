/* Skin colore primario #007a66 */
/* Skin colore secondario #7ebcb1*/
a,a:hover,a:focus {color:#007a66;}
.titFinalita {display:none !important;}

.main-visual {border-top:solid 6px #007a66;}
.main-visual .fascione,.main-visual .promo-box {background: #007a66 ;color:#fff;}
.bordato {border-color:#7ebcb1;}
ul.mTabs {display:block;}
ul.mTabs li a.selected  span {border-color:#7ebcb1; border-bottom:0 none;color:#7ebcb1;}
.checkbox label::after {color:#7ebcb1;}
ul.mTabs li a,ul.mTabs li a:focus {border-bottom: solid 2px #7ebcb1;}
#mCalcCons .sliderBlock .slider-selection, .radio label::after,#mCalcCons .sliderBlock .tooltip.top .tooltip-inner,.mData .mGauge, .btn-grlit, .btn,.btn:active,.btn:hover,.btn:focus  {background-color:#7ebcb1 !important;}
h3.tipomutuo-title {display:none;}
h2.titFinalita {border-color:#7ebcb1; display:none !important;}
h2.titFinalita span {color:#007a66;}

.box-captcha,.box-captcha-right input.form-control {border-color: #7ebcb1 !important;}

/* Sostituzione immagini */
#mCalcCons .plusmin {background-image:url(../../fe/img/green_icon_plus.png);}
#mCalcCons .plusmin.min {background-image: url(../../fe/img/green_icon_min.png);}
.mData .scheda {background-image: url(../../fe/img/green_freccia_link.png);}
#mCalcCons .sliderBlock .slider-handle {background-image: url(../../fe/img/green_mutui_cursore.png);}
a.icoinfo {background-image:url(../../fe/img/green_icon_info.png);}
